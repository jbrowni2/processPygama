{
  "outputs": [
    "channel", "timestamp",
    "bl_intercept", "bl_std",
    "energy", "trapEmax"
  ],
  "processors": {
    "bl_mean1, bl_std1, bl_slope1, bl_intercept1": {
          "function": "linear_slope_fit",
          "module": "pygama.dsp.processors",
          "args": ["waveform[0:100]", "bl_mean1", "bl_std1", "bl_slope1", "bl_intercept1"],
          "unit": ["ADC", "ADC", "ADC", "ADC"],
          "prereqs": ["waveform"]
      },
    "sub1":{
      "function": "bl_subtract",
      "module": "pygama.dsp.processors",
      "args": ["waveform", "bl_intercept1", "sub1"],
      "prereqs": ["waveform", "bl_intercept1"],
      "unit": "ADC"
    },
  "wf_trap": {
    "function": "trap_norm",
    "module": "pygama.dsp.processors",
    "args": ["sub1", "1*us", "0.6*us", "wf_trap"],
    "prereqs": ["sub1"],
    "unit": "ADC"
    },
    "bl_mean, bl_std, bl_slope, bl_intercept": {
            "function": "linear_slope_fit",
            "module": "pygama.dsp.processors",
            "args": ["wf_trap[0:1500]", "bl_mean", "bl_std", "bl_slope", "bl_intercept"],
            "unit": ["ADC", "ADC", "ADC", "ADC"],
            "prereqs": ["wf_trap"]
    },
    "wf_blsub":{
      "function": "bl_subtract",
      "module": "pygama.dsp.processors",
      "args": ["wf_trap", "bl_intercept", "wf_blsub"],
      "prereqs": ["wf_trap", "bl_intercept"],
      "unit": "ADC"
    },
    "tp_min, tp_max, wf_min, trapEmax": {
          "function": "min_max",
          "module": "pygama.dsp.processors",
          "args": ["wf_blsub", "tp_min", "tp_max", "wf_min", "trapEmax"],
          "unit": ["ns", "ns", "ADC", "ADC"],
          "prereqs": ["wf_blsub"]
      }
  }
}
