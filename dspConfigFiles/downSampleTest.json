{
    "outputs": [
        "channel",
        "timestamp",
        "startEnergy",
        "maxEnergy",
        "trapEmax"
    ],
    "processors": {
        "bl_mean1, bl_std1, bl_slope1, bl_intercept1": {
            "function": "linear_slope_fit",
            "module": "pygama.dsp.processors",
            "args": [
                "waveform[10000:15000]",
                "bl_mean1",
                "bl_std1",
                "bl_slope1",
                "bl_intercept1"
            ],
            "unit": [
                "ADC",
                "ADC",
                "ADC",
                "ADC"
            ],
            "prereqs": [
                "waveform"
            ]
        },
        "sub1": {
            "function": "downsampler",
            "module": "pygama.dsp.processors",
            "args": [
                "waveform",
                "bl_slope1",
                "bl_intercept1",
                "sub1"
            ],
            "unit": "ADC"
        },
        "wf_trap": {
            "function": "trap_norm",
            "module": "pygama.dsp.processors",
            "args": [
                "sub1",
                "10*us",
                "0.9*us",
                "wf_trap"
            ],
            "prereqs": [
                "sub1"
            ],
            "unit": "ADC"
        },
        "tp_min, tp_max, wf_min, trapEmax": {
            "function": "min_max",
            "module": "pygama.dsp.processors",
            "args": [
                "wf_trap",
                "tp_min",
                "tp_max",
                "wf_min",
                "trapEmax"
            ],
            "unit": [
                "ns",
                "ns",
                "ADC",
                "ADC"
            ],
            "prereqs": [
                "wf_trap"
            ]
        }
    }
}