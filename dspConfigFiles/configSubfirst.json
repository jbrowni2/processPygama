{
    "outputs": [
        "channel",
        "timestamp",
        "startEnergy",
        "maxEnergy",
        "bl_intercept",
        "bl_std",
        "energy",
        "trapEmax",
        "tp_max0",
        "tp_50",
        "wf_max"
    ],
    "processors": {
        "bl_mean1, bl_std1, bl_slope1, bl_intercept1": {
            "function": "linear_slope_fit",
            "module": "pygama.dsp.processors",
            "args": [
                "waveform[100:15000]",
                "bl_mean1",
                "bl_std1",
                "bl_slope1",
                "bl_intercept1"
            ],
            "unit": [
                "ADC",
                "ADC",
                "ADC",
                "ADC"
            ],
            "prereqs": [
                "waveform"
            ]
        },
        "sub1": {
            "function": "bl_subtract",
            "module": "pygama.dsp.processors",
            "args": [
                "waveform",
                "bl_intercept1",
                "sub1"
            ],
            "prereqs": [
                "waveform",
                "bl_intercept1"
            ],
            "unit": "ADC"
        },
        "tp_min0, tp_max0, wf_min0, wf_max": {
            "function": "min_max",
            "module": "pygama.dsp.processors",
            "args": [
                "sub1",
                "tp_min0",
                "tp_max0",
                "wf_min0",
                "wf_max"
            ],
            "unit": [
                "ns",
                "ns",
                "ADC",
                "ADC"
            ],
            "prereqs": [
                "sub1"
            ]
        },
        "tp_50": {
            "function": "time_point_thresh",
            "module": "pygama.dsp.processors",
            "args": [
                "sub1",
                "wf_max*0.5",
                "tp_max0",
                0,
                "tp_50"
            ],
            "unit": "ns"
        },
        "wf_trap": {
            "function": "trap_norm",
            "module": "pygama.dsp.processors",
            "args": [
                "sub1",
                "6*us",
                "0.0*us",
                "wf_trap"
            ],
            "prereqs": [
                "sub1"
            ],
            "unit": "ADC"
        },
        "tp_min, tp_max, wf_min, trapEmax": {
            "function": "min_max",
            "module": "pygama.dsp.processors",
            "args": [
                "wf_trap",
                "tp_min",
                "tp_max",
                "wf_min",
                "trapEmax"
            ],
            "unit": [
                "ns",
                "ns",
                "ADC",
                "ADC"
            ],
            "prereqs": [
                "wf_trap"
            ]
        }
    }
}